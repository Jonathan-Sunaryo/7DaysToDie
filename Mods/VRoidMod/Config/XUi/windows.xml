	<!-- Common  -->
	<set xpath="/windows/window[@name='CharacterFrameWindow']//panel[@name='avatars']">
		<sprite name="drag" style="drag" pos="218, 10" width="200" height="670"  color="0,0,0,10" globalopacitymod="0.1" />
		<rect pos="-30, -43">
			<avatar_togglebutton pos="-6,128" name="toggleSpringBones" caption_key="SpringBones" width="110" height="28" tooltip_key="toggleSpringBonestt"/>
			<avatar_togglebutton pos="106,128" name="toggleFPAim" caption_key="FP Aim" width="100" height="28" tooltip_key="toggleFPAimtt"/>
			<avatar_togglebutton pos="-6,96" name="toggleFPArms" caption_key="FP Arms" width="110" height="28" tooltip_key="toggleFPArmstt"/>
			<avatar_togglebutton pos="106,96" name="toggleEyewear" caption_key="Eyewear" width="100" height="28" tooltip_key="toggleEyeweartt"/>
			<avatar_togglebutton pos="-6,64" name="toggleTransparency" caption_key="Transparency" width="200" height="28" tooltip_key="toggleTransparencytt"/>
			<avatar_combobox pos="0,30" name="HelmetControls" pivot="center" type="ComboBoxList`1[System.String]" values="Helmet Off,Helmet w/o Hair,Helmet w/ Hair" value_wrap="true" width="200" height="24" visible="true" tooltip_key="HelmetControlstt"/>
			<avatar_combobox pos="0,0" name="ShaderControls" pivot="center" type="ComboBoxList`1[System.String]" values="RealToon,RealToon Self-Lit,Custom Shader" value_wrap="true" width="200" height="24" visible="true" tooltip_key="ShaderControlstt"/>
		</rect>
		<simplebutton name="btnReset" depth="3" pos="0, -75" pivot="center" width="70" height="26" bordercolor="0,0,0,200" defaultcolor="0,0,0,160" hovercolor="255,240,219,210" font_size="20" caption_key="Reset"/>
		<button depth="1" name="changeVoice" style="icon32px, press, hover" width="24" height="24" pivot="center" sprite="ui_game_symbol_talk" pos="60,-88" tooltip="Change Voice" sound="[paging_click]" hoverscale="1.0"/>
		<pager name="pager" pos="80,-73"/>
		<grid name="avatars" pos="0, -143" rows="8" cols="3" cell_width="70" cell_height="70" repeat_content="true">
			<avatar_entry width="70"/>
		</grid>
		<label depth="2" name="version" pos="240,-654" width="200" height="32" text_key="vroid_version" justify="right" font_size="16"/>
		<sprite depth="0" name="bg" sprite="menu_empty3px" pos="-44,94" width="227" height="770" color="[black]" type="sliced" fillcenter="false"/>
		<sprite depth="0" name="bg" sprite="menu_empty3px" pos="-44,94" width="227" height="770" color="[darkGrey]" type="sliced"/>
	</set>
	<remove xpath="/windows/window[@name='windowRadial']//label[@name='selection']"/>
	<append xpath="/windows/window[@name='windowRadial']/rect[@name='radialPanel']">
		<label depth="10" name="selection" width="500" height="500" pivot="center" justify="center" text=" " effect="outline" effect_distance="2,2" effect_color="0,0,0,150" font_size="26"/>
	</append>
